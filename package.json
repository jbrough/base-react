{
  "name": "base-react",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prestart": "npm run build",
    "start": "NODE_ENV=development node --use_strict server/server",
    "test": "tape -r babel-register test/server/**/*.js test/client/**/*.jsx",
    "lint": "eslint server test || true",
    "build": "rm -rf dist && mkdir -p dist/js && npm run _mv_static && npm run _browserify",
    "_mv_static": "cp -rf client/static/css client/static/img client/static/html/* dist/",
    "_browserify": "browserify --extension=.jsx client/src/js/index.js -o dist/js/bundle.js -t [ babelify ]"
  },
  "author": "Jamie Brough <jamie1612@gmail.com>",
  "license": "ISC",
  "engines": {
    "node": "5.11"
  },
  "eslintConfig": {
    "extends": "airbnb/base"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "bunyan": "^1.8.1",
    "bunyan-request": "^1.2.0",
    "express": "^4.13.4",
    "express-request-id": "^1.1.0",
    "js-yaml": "^3.6.1",
    "react": "^15.1.0",
    "react-addons-update": "^15.1.0",
    "react-dom": "^15.1.0",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6",
    "superagent": "^2.0.0-alpha.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-react": "^5.1.1",
    "react-addons-test-utils": "^15.1.0",
    "skin-deep": "^0.16.0",
    "supertest": "^1.2.0",
    "tape": "^4.5.1",
    "vinyl-source-stream": "^1.1.0"
  }
}
